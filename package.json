{
  "name": "nextjs-graphql-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "dev": "next dev",
    "format": "npm run format:ts && npm run format:js && npm run format:json && npm run format:yaml",
    "format:ts": "prettier --parser typescript --write \"**/*.{ts,tsx}\"",
    "format:js": "prettier --write \"**/*.js\"",
    "format:json": "prettier --parser json --write \"**/*.{json,prettierrc}\"",
    "format:yaml": "prettier --parser yaml --write \"**/*.{yml,yaml}\"",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:ci": "npm run lint --format junit -o __reports__/eslint-results.xml",
    "lint:fix": "npm run lint --fix",
    "lint:quiet": "npm run lint --quiet",
    "precommit": "lint-staged",
    "start": "next start",
    "test": "jest --config jest.config.js --passWithNoTests",
    "test:watch": "npm run test --watch",
    "typecheck": "tsc --noEmit",
    "generate:schema": "graphql-codegen --config codegen.schema.yml",
    "generate:api": "graphql-codegen --config codegen.api.yml"
  },
  "dependencies": {
    "@apollo/client": "^3.8.0-rc.2",
    "@apollo/experimental-nextjs-app-support": "^0.4.1",
    "@effector/next": "^0.5.1",
    "@justoss/code-style": "^0.1.2",
    "@seed-ui/react": "2.0.2",
    "@seed-ui/styles": "2.0.3",
    "@vanilla-extract/next-plugin": "^2.3.0",
    "axios": "^1.5.0",
    "classnames": "2.3.1",
    "effector-react": "^22.5.3",
    "lodash": "^4.17.21",
    "next": "13.4.19",
    "next-auth": "^4.23.1",
    "next-compose-plugins": "^2.2.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.45.4",
    "react-use": "^17.4.0",
    "typescript": "4.9.5",
    "urlcat": "^3.1.0",
    "use-deep-compare-effect": "^1.8.1"
  },
  "devDependencies": {
    "@graphql-codegen/add": "^3.1.1",
    "@graphql-codegen/cli": "^2.3.0",
    "@graphql-codegen/near-operation-file-preset": "^2.2.3",
    "@graphql-codegen/schema-ast": "^2.4.1",
    "@graphql-codegen/typescript": "^2.4.1",
    "@graphql-codegen/typescript-operations": "^2.2.1",
    "@graphql-codegen/typescript-react-apollo": "^3.2.4",
    "@types/lodash": "^4.14.197",
    "@types/node": "20.5.6",
    "@types/query-string": "^6.3.0",
    "@types/react": "18.2.21",
    "@types/react-dom": "18.2.7",
    "@typescript-eslint/parser": "^5.62.0",
    "eslint": "8.48.0",
    "eslint-config-next": "13.4.19",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-perf": "^3.3.1",
    "immer": "^10.0.2",
    "prettier": "^3.0.3"
  }
}
